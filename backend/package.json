{
  "name": "soumaya-beauty-bar",
  "version": "1.0.0",
  "type": "module",
  "description": "Sistema de reservas para Soumaya Beauty Bar con Google Calendar y WhatsApp",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "sync-calendar": "node -e \"import('./src/utils/syncGoogleCalendar.js').then(m => m.syncGoogleCalendar()).catch(console.error)\"",
    "verify-integrity": "node -e \"import('./src/utils/syncGoogleCalendar.js').then(m => m.verificarIntegridad()).catch(console.error)\"",
    "test-webhook": "node -e \"console.log('Webhook URL: https://soumaya-beauty-salon.onrender.com/api/whapi/webhook')\""
  },
  "keywords": [
    "beauty-salon",
    "reservations",
    "google-calendar",
    "whatsapp",
    "whapi"
  ],
  "author": "Soumaya Beauty Bar",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "googleapis": "^128.0.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}